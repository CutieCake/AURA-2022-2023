float seaAltitude; // altitude compared to sea level
bool appogee; // binary statement of appogee (false is not at appogee*)
float initAltitude; // initial altitude compared to the sea level
float altitudeTrue; // actual altitude compared to ground
float prevAltitude; // previous altitude measurement 
float velocity; // speed of rocket in -z direction
void setup() {
  // put your setup code here, to run once:
 seaAltitude == 0 ;
 analogRead(initAltitude); //read initial ground level altitude in sea level
}

void loop() {
  // put your main code here, to run repeatedly:
    analogRead(seaAltitude); //read sea level altitude

   altitudeTrue = seaAltitude - initAltitude; // get altitude  in relation to ground from the sea level altitude

   velocity = (altitudeTrue - prevAltitude)/ .1; // take previous altitude reading and calculate speed in z - speed in ft/s

   if (0 <= velocity <= 1){

   appogee = 1;
   
   }
     prevAltitude = altitudeTrue;

   delay(100);
   
   
     }
